PUT http://localhost:8083/api/songs/ffdffd02-505c-4d1c-af3d-f3555c9b9827
Content-Type: application/json
Authorization: Basic a2V2aW46dXNlcnVzZXI=

{
  "name": "Moves Like Jagger",
  "releaseYear": 2010,
  "singer": "ffdffd02-505c-4d1c-af3d-f3555c9b9829"
}

